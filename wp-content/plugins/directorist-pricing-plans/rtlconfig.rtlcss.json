{
    "options": {
        "autoRename": false,
        "autoRenameStrict": false,
        "blacklist":{},
        "clean": true,
        "greedy": false,
        "processUrls": false,
        "stringMap":[
            {
                "name": "left-right",
                "priority": 100,
                "search": ["left", "Left", ""],
                "replace": ["right", "Right", "RIGHT"],
                "options": {
                    "scope": "*",
                    "ignoreCase": false
                }
            },
            {
                "name": "ltr-rtl",
                "priority": 100,
                "search": ["ltr", "Ltr", "LTR"],
                "replace": ["rtl", "Rtl", "RTL"],
                "options": {
                    "scope": "*",
                    "ignoreCase": false
                }
            }
        ],
        "useCalc": false,
        "aliases":{}
    },
    "plugins": [ ],
    "map": false
}